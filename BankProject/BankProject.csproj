<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\IconClose.png" />
    <None Remove="Images\IconEmail.png" />
    <None Remove="Images\IconFacebook.png" />
    <None Remove="Images\IconGoogle.png" />
    <None Remove="Images\IconLinkedin.png" />
    <None Remove="Images\IconLock.png" />
    <None Remove="Images\IconMinimize.png" />
    <None Remove="Images\IconUser.png" />
  </ItemGroup>

  <ItemGroup>
